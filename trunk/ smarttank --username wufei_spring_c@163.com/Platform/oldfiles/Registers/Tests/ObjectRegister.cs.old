using System;
using System.Collections.Generic;
using System.Text;

namespace Platform.Logic.Registers
{
    public class ObjectRegister<ObjectType> : Register<ObjectType>
    {
        protected ObjectRegister ()
        {
            this.container = new List<ObjectType>();
        }

        public static ObjectRegister<ObjectType> getInstance ()
        {
            if (instance == null) instance = new ObjectRegister<ObjectType>();
            return instance;
        }

        protected static ObjectRegister<ObjectType> instance;
    }
}
